{
  "name": "rds_visualizer",
  "title": "rds_visualizer",
  "summary": "RDS visualizing webapp",
  "httpsApp": {
    "ports": [
      443
    ],
    "shared_access": "VIEW",
    "dns": {}
  },
  "dxapi": "1.0.0",
  "inputSpec": [],
  "outputSpec": [],
  "runSpec": {
    "interpreter": "bash",
    "timeoutPolicy": {
      "*": {
        "hours": 48
      }
    },
    "headJobOnDemand": false,
    "inheritParentRestartOnPolicy": false,
    "distribution": "Ubuntu",
    "release": "20.04",
    "version": "0",
    "file": "src/code.sh"
  },
  "access": {
    "network": [
      "*"
    ],
    "project": "CONTRIBUTE"
  },
  "ignoreReuse": false,
  "regionalOptions": {
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x4"
        }
      }
    }
  }
}
