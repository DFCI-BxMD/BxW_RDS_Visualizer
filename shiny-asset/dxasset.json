{
    "name": "shiny_asset",
    "title": "Shiny Asset",
    "description": "Libraries for building web apps with R Shiny",
    "version": "0.0.1",
    "distribution": "Ubuntu",
    "release": "20.04",
    "execDepends": [
        {"name": "shiny",
            "package_manager": "cran"},
        {"name": "shinydashboard",
            "package_manager": "cran"},
        {"name": "shinydashboardPlus",
            "package_manager": "cran"},
        {"name": "purrr",
            "package_manager": "cran"},
        {"name": "stringr",
            "package_manager": "cran"},
        {"name": "DT",
            "package_manager": "cran"},
        {"name": "shinyalert",
            "package_manager": "cran"},
        {"name": "ggplot2",
            "package_manager": "cran"},
        {"name": "remotes",
            "package_manager": "cran"},
        {"name": "Seurat",
            "package_manager": "cran"},
        {"name": "seurat-disk",
                 "package_manager": "git",   
                 "url": "https://github.com/mojaveazure/seurat-disk",
                 "tag": "master",
                 "build_commands": "R --no-echo --no-restore --no-save -e remotes::install_github('mojaveazure/seurat-disk')"
        }
    ]
}
